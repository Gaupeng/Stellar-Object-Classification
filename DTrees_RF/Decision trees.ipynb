    "            count1 += 1\n",
    "        elif(classification == 2):\n",
    "            count2 += 1\n",
    "    \n",
    "    label = [count0,count1,count2]\n",
    "    \n",
    "    \n",
    "    fin_class = label.index(max(label))\n",
    "    return (fin_class)"
